type pair as rec(
  var x = 0 as int,
  var y = 0 as int
)

method ()(pair, var x as int, var y as int) as pair
{
  var res as pair;
  res.x = x;
  res.y = y;
  return res;
}

method del(var this as ref(pair))
{
  this.x = 0;
  this.y = 0;
  echo "deleted";
}

sub f() as pair
{
  p = pair(1, 3);
  q = pair(2, 5);
  return p;
}

entry
{
  exec f();
  echo "end";
}