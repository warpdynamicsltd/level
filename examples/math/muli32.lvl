sub check(var a as i32, var b as i32) as u32
{
  if (sgn(a) * sgn(b) * abs(a) * abs(b) != a * b)
  {
    return 1;
  }

  if (abs(a) != sgn(a) * a)
  {
    return 2;
  }

  if (a*a - 2*a*b + b*b < 0)
  {
    return 3;
  }

  if (a * a < 0)
  {
    return 4;
  }

  if (abs(a) * abs(b) < 0)
  {
    return 5;
  }

  if ((-abs(a))*abs(b) > 0)
  {
    return 6;
  }

  if (abs(a)*(-abs(b)) > 0)
  {
    return 7;
  }

  if ((a + b)*(a + b) != a*a + 2*a*b + b*b)
  {
    return 8;
  }

  if ((a - b)*(a + b) != a*a - b*b)
  {
    return 8;
  }

  if ((a - b)*(a - b) != a*a - 2*a*b + b*b)
  {
    return 8;
  }


  return 0;
}

sub massive_check
{
  var r as ref(i32);
  var N as u32;

  var sum as u32;
  sum = 0;

  for (i = 0; i < N; inc i)
  {
    for (j = 0; j < N; inc j)
    {
      k = check(r[j], r[i]);
      if (k > 0)
      {
        echo k;
        echo r[j];
        echo r[i];
        return 1;
      }
      sum = sum + check(r[j], r[i]);
    }
  }

  return sum;
}

entry
{
  var a as array(i32, 10);
  a[0] = 9;
  a[1] = 2123;
  a[2] = 2323;
  a[3] = 0;
  a[4] = -a[0];
  a[5] = -a[1];
  a[6] = -a[2];
  echo massive_check(ref a, 7) == 0;
}