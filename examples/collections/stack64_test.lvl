import stdlib:collections:stack64 as *;

entry{
  s = new(stack64);

  var payload as u64;

  N = 10000;

  for (k = 1; k < N; inc k)
  {
    payload = k;
    exec s.push(payload);
  }

  var k as u64;
  k = N - 1;

  var res as bool;
  res = true;

  while (true)
  {
    payload = s.pop();
    if (payload == 0) {break}
    res = (payload == k);
    dec k;
  }

  for (k = 1; k < N; inc k)
  {
    payload = k;
    exec s.push(payload);
  }

  echo res;

  k = N - 1;

  while (true)
  {
    payload = s.pop();
    if (payload == 0) {break}
    res = (payload == k);
    dec k;
  }

  echo res;
}