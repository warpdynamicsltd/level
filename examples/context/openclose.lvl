import stdlib:sys:context as *;
import stdlib:sys:app as *;

sub f()
{
  a = mem_obj();
  b = mem_obj();
  c = mem_obj();

  echo "referenced 1";
  echo mem_obj(10).m.ref_count;

  a = mem_obj(10);
  echo "referenced 2";
  echo a.m.ref_count;
  b = a;
  c = a;
  echo "referenced 4";
  echo a.m.ref_count;
}

entry{
    exec f();
    exec gc.collect();
    exec gc.clear();
    echo "gc test";
    echo app.allocator.used;
    echo "end";
}