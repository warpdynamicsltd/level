import stdlib:sys:context as *;
import stdlib:sys:app as *;
import object:collections:vector as *;

sub f()
{
  a = vector(int)(1);
  b = vector(int)(1);
  b = vector(int)(16);
  a = vector(int)(10);
  b = a;
  exec vector(int)(5);
  a = vector(int)(10);
}

entry{
    exec f();

    exec gc.collect();

    echo "global stack length";
    echo gc.global_stack.length;

    exec gc.clear();

    echo "gc test";
    echo app.allocator.used;
    echo "end";
}